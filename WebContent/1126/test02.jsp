<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<h1> ${10 + 10}</h1>
	<h1> ${10 * 10}</h1>
	
	<!-- 영문 비교연산자는 xml에서 주로 사용 -->
	<h1> ${10 > 20}</h1>
	<h1> ${10 gt 20}</h1>
	
	<h1> ${10 < 20}</h1>
	<h1> ${10 lt 20}</h1>
</body>
</html>